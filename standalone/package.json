{
    "name": "core-standalone",
    "author": "ABR Tech",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "build": "tsc & webpack",
        "build:watch": "tsc -w & webpack --watch",
        "start": "electron ./install_app/build",
        "backend:clone": "git clone https://github.com/essence-community/core-backend.git",
        "backend:install": "cd ./core-backend && yarn",
        "backend:build:plugins": "cd ./core-backend && gulp plugins",
        "backend:build:contexts": "cd ./core-backend && gulp contexts",
        "backend:build:events": "cd ./core-backend && gulp events",
        "backend:build:schedulers": "cd ./core-backend && gulp schedulers",
        "backend:build:providers": "cd ./core-backend && gulp providers",
        "backend:build:server": "cd ./core-backend && gulp server",
        "backend:build:plugininf": "cd ./core-backend && gulp plugininf",
        "backend:build:libs": "cd ./core-backend && gulp libs",
        "backend:build:cert": "cd ./core-backend && gulp cert",
        "backend:build:copy": "cd ./core-backend && gulp copy",
        "frontend:clone": "git clone https://github.com/essence-community/core-frontend.git",
        "frontend:install": "cd ./core-frontend && yarn",
        "frontend:build": "cd ./core-frontend && yarn build"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.6.0",
        "@flow-ui/core": "^0.1.3",
        "@types/node": "^12.11.2",
        "@types/pg": "^7.11.2",
        "@types/react": "^16.9.9",
        "@types/react-dom": "^16.9.2",
        "babel-loader": "^8.0.6",
        "electron": "^7.0.0",
        "gulp": "^4.0.0",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.0",
        "liquibase": "^0.0.3",
        "pg": "^7.12.1",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.4",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9"
    },
    "dependencies": {
        "rimraf": "^3.0.0"
    }
}
