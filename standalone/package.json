{
    "name": "core-standalone",
    "author": "ABR Tech",
    "version": "0.1.0",
    "license": "MIT",
    "engines": {
        "yarn": ">=1.13.0",
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "tsc & webpack",
        "build:watch": "tsc -w & webpack --watch",
        "lint": "eslint --config .eslintrc.js --ext ts,tsx,js,jsx src",
        "lint:fix": "eslint --config .eslintrc.js --ext ts,tsx,js,jsx --fix src",
        "start": "electron ./build",
        "backend:install": "cd ../backend && git clean -fdx && yarn",
        "backend:build": "cd ../core-backend && yarn build",
        "frontend:install": "cd ../frontend && git clean -fdx && yarn",
        "frontend:build": "cd ../core-frontend && yarn build"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.6.0",
        "@flow-ui/core": "^0.1.32",
        "@types/adm-zip": "^0.4.32",
        "@types/node": "^12.12.27",
        "@types/pg": "^7.11.2",
        "@types/react": "^16.9.9",
        "@types/react-dom": "^16.9.2",
        "@types/webpack": "^4.41.6",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "babel-loader": "^8.0.6",
        "electron": "^7.0.0",
        "electron-packager": "^14.2.1",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "html-webpack-plugin": "^3.2.0",
        "prettier": "^1.19.1",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.5",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9"
    },
    "dependencies": {
        "adm-zip": "^0.4.14",
        "copy-dir": "^1.2.0",
        "pg": "^7.12.1"
    }
}
